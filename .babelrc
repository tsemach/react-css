{
  "presets": [
    ["env", 
      {
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        }
      }
    ],
    "react",  
    "@babel/preset-env", 
    "@babel/preset-react"
  ],
  
  "plugins": [
    "transform-class-properties", 
    "@babel/plugin-proposal-class-properties",  
    "transform-es2015-classes", "transform-regenerator",
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ]
}

